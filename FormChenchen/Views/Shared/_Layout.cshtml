<!DOCTYPE html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=0.8">
    <title>VuelveElChenchen</title>

    <!-- Estilos -->
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true">
    <link rel="stylesheet" href="~/css/style.css" asp-append-version="true">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">


</head>
<body class="background-container"> 
    <!-- Scripts -->  

    <div class="image-container"> 
            <img src="..\img\logo.png" alt="Logo" class="logo-chen">
    </div>

    <div class="postion-dollars dollars">
        <div class="col col-12 row p-0 m-0">
            <div class="col col-12 col-lg-9 col-xl-6 col-md-11 shadow-lg container container-form rounded-4 py-2 p-0 p-md-3 p-lg-3 mt-3 d-flex flex-column justify-content-center">
                <main role="main" class="pb-3">
                    @RenderBody()
                </main>
            </div>
        </div>
        <div id="imagenpie" class="gobernment position-absolute">
            <div class="col col-12 d-flex justify-content-center justify-content-lg-end flex-wrap px-4">
                <img src="~/img/gobierno nacional.png" alt="" class="my-3" style="width: 300px;">
                <img src="~/img/caja de ahorros.png" alt="" class="my-3 ms-3" style="width: 215px;">
            </div>
        </div>
    </div>

    
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.mask/1.14.16/jquery.mask.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/autonumeric@4.6.0/dist/autoNumeric.min.js"></script> 
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script> 
 

    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>

 

<div class="toast-container position-fixed top-0 end-0 p-3" style="z-index: 1055;">
    <div id="alertToast" class="toast align-items-center text-white bg-danger border-0" role="alert" aria-live="assertive" aria-atomic="true">
        <div class="d-flex">
            <div id="toastBody" class="toast-body">
                <!-- El mensaje se llenará por JavaScript -->
            </div>
            <button type="button" class="btn-close btn-close-white me-2 m-auto" data-bs-dismiss="toast" aria-label="Cerrar"></button>
        </div>
    </div>
</div>



</html>
