@{
    Layout = "_Layout";
    ViewData["Title"] = "Reporte";
}
 
 
    <div class="container">  
                <h2 class="mb-4">Lista de Reportes</h2>

                <div class="card report-custom p-4 mb-4">
                    <div class="row mb-3 align-items-center">
                         <div class="col-md-4">
                            <label class="form-label">Tipo de reporte</label>
                            <select class="form-select">
                                <option>Formulario Registrados</option> 
                            </select>
                          </div>
                         
                    </div>

                    <hr>

                    <!-- Filtros -->
                    <h5>Filtros</h5>
                    <div class="row g-3 align-items-end">
                        <div class="col-md-4">
                            <label class="form-label">Fecha de creación</label>
                            <div class="input-group">
                                <input id="startDate" class="form-control" type="date" aria-label="Fecha Inicio">
                                <span class="input-group-text">a</span> 
                                <input id="endDate" class="form-control" type="date" aria-label="Fecha Fin">
                            </div>
                        </div>
                       
                        <div class="col-md-4 text-end">
                            <button class="btn btn-primary" onclick="GenereReporte()">Descargar</button>
                        </div>
                    </div>

                </div>
            </div>
 

 